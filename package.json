{
  "name": "mooncellar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 10",
    "lint:fix": "eslint src --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,html}'",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run format"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "axios": "^1.9.0",
    "classnames": "^2.5.1",
    "interweave": "^13.1.1",
    "jwt-decode": "^4.0.0",
    "next": "^15.3.3",
    "query-string": "^9.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-markdown": "^10.1.0",
    "react-resize-detector": "^12.0.2",
    "react-scan": "^0.3.4",
    "react-spinners": "^0.17.0",
    "sass": "^1.89.2",
    "scss": "^0.2.4",
    "typescript-eslint": "^8.34.0",
    "use-debounce": "^10.0.5",
    "zod": "^3.25.58",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify/react": "^6.0.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5"
  }
}
